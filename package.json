{
	"name": "musee",
	"type": "module",
	"version": "1.0.0",
	"private": true,
	"sideEffects": false,
	"author": "Ayaan Zaidi (hi@obviy.us)",
	"license": "GNU GPLv3",
	"description": "Self hosted gallery written in React + Astro. Ships minimal JS to the client.",
	"scripts": {
		"predev": "bun run imageImporter.ts",
		"dev": "bunx --bun astro dev",
		"prebuild": "bun run imageImporter.ts",
		"build": " bunx --bun astro build",
		"preview": "bunx --bun astro preview",
		"astro": "bunx --bun astro"
	},
	"dependencies": {
		"@astrojs/sitemap": "^3.2.1",
		"@tailwindcss/vite": "^4.1.11",
		"astro": "5.12.4",
		"astro-critters": "^2.2.1",
		"exifreader": "^4.31.1",
		"luxon": "^3.7.1",
		"sharp": "^0.34.3",
		"sqids": "^0.3.0",
		"tailwindcss": "^4.1.11"
	},
	"repository": "git@github.com:obviyus/musee.git",
	"devDependencies": {
		"@types/bun": "^1.2.19",
		"@types/luxon": "^3.7.1",
		"tsx": "^4.20.3"
	},
	"release": {
		"branches": [
			"master"
		],
		"plugins": [
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/npm",
			"@semantic-release/git",
			"@semantic-release/github"
		]
	}
}
